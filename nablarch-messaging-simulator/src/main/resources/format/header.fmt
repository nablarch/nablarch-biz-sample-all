#====================================================================
# フレームワーク制御ヘッダ部 (50byte)
#====================================================================
file-type:        "Fixed" # 固定長
text-encoding:    "MS932" # 文字列型フィールドの文字エンコーディング
record-length:    50      # レコード長
record-separator: "\r\n"  # 改行コード(crlf)

[header]
1   requestId     X(20)   # リクエストID
21  resendFlag    X(1)    # 再送要求フラグ (0: 初回送信 1: 再送要求)
22  statusCode    X(8)    # ステータスコード
30  userId        X(10)   # 送信ユーザID
40  ?filler       X(11)   # 予備領域
