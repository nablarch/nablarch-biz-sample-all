<?xml version="1.0" encoding="UTF-8"?>
<component-configuration
    xmlns="http://tis.co.jp/nablarch/component-configuration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://tis.co.jp/nablarch/component-configuration">

  <!-- FormatterFactoryの設定 -->
  <component name="formUrlEncodedDataConvertorSetting"
      class="please.change.me.core.dataformat.convertor.FormUrlEncodedDataConvertorSetting">
      <property name="convertorTable">
          <map>
              <entry key="X" value="nablarch.core.dataformat.convertor.datatype.NullableString"/>
              <entry key="N" value="nablarch.core.dataformat.convertor.datatype.NullableString"/>
              <entry key="XN" value="nablarch.core.dataformat.convertor.datatype.NullableString"/>
              <entry key="X9" value="nablarch.core.dataformat.convertor.datatype.NullableString"/>
              <entry key="SX9" value="nablarch.core.dataformat.convertor.datatype.NullableString"/>
              <entry key="_LITERAL_" value="nablarch.core.dataformat.convertor.value.DefaultValue"/>
              <entry key="number" value="nablarch.core.dataformat.convertor.value.ExponentialNumberString"/>
              <entry key="signed_number" value="nablarch.core.dataformat.convertor.value.ExponentialSignedNumberString"/>
              <entry key="replacement" value="nablarch.core.dataformat.convertor.value.CharacterReplacer"/>
          </map>
      </property>
  </component>
  <component name="formatterFactory" class="please.change.me.core.dataformat.FormUrlEncodedDataFormatterFactory"/>
</component-configuration>
