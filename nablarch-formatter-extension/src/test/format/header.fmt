#====================================================================
# フレームワーク制御ヘッダ部 (50byte)
#====================================================================
file-type:        "Fixed" # 固定長
text-encoding:    "MS932" # 文字列型フィールドの文字エンコーディング
record-length:    40      # レコード長
record-separator: "\r\n"  # 改行コード(crlf)

[header]
1   requestId     X(30)       # リクエストID
31  resendFlag    X(1)        # 再送要求フラグ (0: 初回送信 1: 再送要求)
32  ?filler       X(9)        # 予備領域
